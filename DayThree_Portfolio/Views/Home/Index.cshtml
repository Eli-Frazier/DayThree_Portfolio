    <!-- ModalFactorial -->
    <div id="ModalFactorial" class="modal fade" role="dialog">
        <div class="modal-dialog modal-lg">

            <!-- Modal content-->
            <div class="modal-content">
                <!--Header-->
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h1>Factorial Frenzy</h1>
                </div>

                <!--Body-->
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-6">Please enter your number</div>
                                <div class="col-md-6"> <input id="numfac" type="number" class="form-control" /></div>
                            </div>
                            <div class="row">
                                <div class="col-md-4">
                                    <button id="btnFac" type="button" class="btn btn-primary">Show Factorial</button>
                                </div>
                                <div class="col-md-6 text-right" style="font-size: 15px; padding-top: 5px">
                                    Toggle Code
                                </div>
                                <div class="col-md-2">
                                    <i id="toggleFac" class="fa fa-toggle-off fa-align-left" style="font-size:36px;color:royalblue;"></i>
                                </div>
                            </div>
                        </div>
                        <div class="col=md-6 text-center">
                            <div class="row" style="padding-top: 10px">
                                <p id="outputfac"></p>
                            </div>
                        </div>
                        <div id="codeFac" class="row">
                            <div class="col-md-12">
                                <pre class="brush: js">
                                      $("#btnFac").click(function () {
                                            //gain user input
                                            var usernum = $("#numfac").val();

                                            //do the math to find the factorial
                                            var numFac = usernum;
                                            for (var loop = usernum - 1; loop >= 1; loop--) {
                                                numFac = numFac *= loop;
                                            }

                                            //output the factorial
                                            $("#outputfac").html("the factorial of <b> " + usernum + "</b> is <b>" + numFac + "</b>");
                                        });
                            </pre>
                            </div>

                        </div>
                    </div>
                </div>

                <!--Footer-->
                <div class="modal-footer">
                    <div class="form-group">
                        <div class="col-sm-offset-2 col-sm-10">
                            <button id="close" type="button" class="btn btn-primary btn-lg" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

