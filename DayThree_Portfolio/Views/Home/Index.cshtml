                                </div>
                            </div>
                        </div>
                        <div class="col-md-6 text-center">
                            <div class="row" style="padding-top: 10px">
                                <p id="outputpal"></p>
                            </div>
                        </div>
                        <div id="codePal" class="row">
                            <div class="col-md-12">
                                <pre class="brush: js">
                                    $("#btnPalindrome").click(function () {
                                        var userword = $("#word1").val();

                                        revword = userword.split("").reverse().join("");

                                        if (userword.toUpperCase() === revword.toUpperCase()) {
                                            $("#outputpal").html("<b>" + userword + "</b> is a palindrome");
                                        }
                                        else {
                                            $("#outputpal").html("<b>" + userword + "</b> is not a palindrome");
                                        }
                                    });
                            </pre>
                            </div>

                        </div>
                    </div>
                </div>

                <!--Footer-->
                <div class="modal-footer">
                    <div class="form-group">
                        <div class="col-sm-offset-2 col-sm-10">
                            <button id="close" type="button" class="btn btn-primary btn-lg" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
