    <!-- ModalFizzBuzz -->
    <div id="ModalFizzBuzz" class="modal fade" role="dialog">
        <div class="modal-dialog modal-lg">

            <!-- Modal content-->
            <div class="modal-content">
                <!--Header-->
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h1>FizzBuzz</h1>
                </div>

                <!--Body-->
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-6">Please enter a fizz number</div>
                                <div class="col-md-6"> <input id="fizz" type="number" class="form-control" /></div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">Please enter a buzz number</div>
                                <div class="col-md-6"> <input id="buzz" type="number" class="form-control" /></div>
                            </div>
                            <div class="row">
                                <div class="col-md-4">
                                    <button id="btnFB" type="button" class="btn btn-primary">Fizz Buzz</button>
                                </div>
                                <div class="col-md-6 text-right" style="font-size: 15px; padding-top: 5px">
                                    Toggle Code
                                </div>
                                <div class="col-md-2">
                                    <i id="toggleFB" class="fa fa-toggle-off fa-align-left" style="font-size:36px;color:royalblue;"></i>
                                </div>
                            </div>
                        </div>

                        <div class="col=md-6">
                            <div class="row" style="padding-top: 10px">
                                <div class="col-md-6">
                                    <p id="outputFB"></p>
                                </div>
                                
                            </div>
                        </div>
                    </div>
                    <div id="codeFB" class="row">
                            <div class="col-md-12">
                                <pre class="brush: js">
                                        $("#btnFB").click(function () {
                                            //gain user input
                                            var fizz = $("#fizz").val();
                                            var buzz = $("#buzz").val();

                                            //generate the output (somehow)
                                            //if a number (1-100) is a multiple of the fizz number, display fizz (loop % fizz = 0)
                                            //elseif the number is a multiple of the buzz number, display buzz (loop % buzz = 0)
                                            //elseif the number is a multiple of both, display fizzbuzz (loop % fiiz and buzz = 0)
                                            //else, display the number
                                            var outputarray = [];
                                            for (var loop = 1; loop <= 100; loop++) {

                                                if (loop % fizz === 0 && loop % buzz === 0) {
                                                    outputarray.push("<span class='BoldPurple'>FizzBuzz</span>")
                                                }
                                                else if (loop % fizz === 0) {
                                                    outputarray.push("<span class='BoldRed'>Fizz</span>")
                                                }
                                                else if (loop % buzz === 0) {
                                                    outputarray.push("<span class='BoldBlue'>Buzz</span>")
                                                }
                                                else {
                                                    outputarray.push(loop)
                                                }
                                            }

                                            //output the factorial
                                            $("#outputFB").html(outputarray.join(", "));
                                        });

                                </pre>
                            </div>
                    </div>            
                </div>

                <!--Footer-->
                <div class="modal-footer">
                    <div class="form-group">
                        <div class="col-sm-offset-2 col-sm-10">
                            <button id="close" type="button" class="btn btn-primary btn-lg" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
